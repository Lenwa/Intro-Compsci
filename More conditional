			System.out.println("Yes, x=y");
		else 
			System.out.println("No they are not equal");
		
		
		//use .equals to compare objects, objects compare addresses
		if(a.equals(b))
		//this won't give the right answer
		//if(a == b)
			System.out.println("Yes, x=y");
		else 
			System.out.println("No they are not equal");
		
		//convert all characters to lowercase 
